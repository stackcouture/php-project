{
  "name": "docker/php-app",
  "description": "Simple PHP App in Docker with AWS Secrets Manager",
  "require": {
    "vlucas/phpdotenv": "^5.5",
    "aws/aws-sdk-php": "^3.300"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer dump-autoload"
    ],
    "post-update-cmd": [
      "@composer dump-autoload"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
